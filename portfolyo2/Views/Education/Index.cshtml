@model List<portfolyo2.Models.tbl_education>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Burası EĞİTİM SAYFAI</h2>
<div class="container">
    <table class="table table-bordered"
               <thead>
                   <tr>
                      <th>eğitim ID</th>
                   </tr>
               </thead>
               <tbody>
                   @foreach (var item in Model)
            {
                <tr>
                    <td>@item.EducationID</td>
                </tr>
            }


               </tbody>
    </table>
</div>
