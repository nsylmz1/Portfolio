@model List<portfolyo2.Models.>
<div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
    <h3 class="resume-title"> Experience</h3>

    @foreach (var item in Model)
    {
        <div class="resume-item">
            <h4>@item.Tittle</h4>
            <h5>

                @( @item.StartDate.HasValue ? @item.StartDate.Value.ToString("MM/yyyy") : "")
                -
                @( @item.EndDate.HasValue ? @item.EndDate.Value.ToString("MM/yyyy") : "Present")
            </h5>
            <p><em>@item.CompanyName </em></p>
            <ul>
                <li>@item.Description</li>

            </ul>
        </div>
    }



</div>


