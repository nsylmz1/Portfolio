@model List<portfolyo2.Models.tbl_education>

<div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">

    <h3 class="resume-title">Education</h3>
    @foreach (var item in Model)
    {

        <div class="resume-item">
            <h4>@item.Name</h4>
            <h5>
               @( @item.StartYear.HasValue ? @item.StartYear.Value.ToString("MM/yyyy") : "")

                -
              @( @item.EndeYear.HasValue ? @item.EndeYear.Value.ToString("MM/yyyy") : "Present")
            </h5>
            <p><em>@item.Section</em></p>
            <p>@item.Description</p>
        </div><!-- Edn Resume Item -->
    }


</div>

