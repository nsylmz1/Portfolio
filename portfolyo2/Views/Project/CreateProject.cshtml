
@{
    ViewBag.Title = "CreateProject";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>CreateProject</h2>

<form method="post">
    <label>Project Name</label>
    <input type="text" class="form-control" name="ProjectName" />
    <label>Description</label>
    <input type="text" class="form-control" name="Description" />
    <label>Project Link</label>
    <input type="text" class="form-control" name="ProjectLink" />
    <label>Image1</label>
    <input type="text" class="form-control" name="image1" />
    <label>Image2</label>
    <input type="text" class="form-control" name="image2" />
    <label>Image3</label>
    <input type="text" class="form-control" name="image3" />
   
    <button type="submit" style="background-color:orange">Oluştur</button>
</form>

